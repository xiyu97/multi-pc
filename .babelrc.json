{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "usage",
                // "modules": false,
                "corejs": 3,
                "targets": {
                    "edge": "17",
                    "ie": "8",
                    "firefox": "60",
                    "chrome": "67",
                    "safari": "11.1"
                }
            }
        ]
    ],
    "plugins": [
        "@babel/plugin-transform-runtime",
        "@babel/plugin-transform-modules-commonjs",
        "@babel/plugin-transform-object-assign",
        "transform-es2015-modules-simple-commonjs"
    ]
}